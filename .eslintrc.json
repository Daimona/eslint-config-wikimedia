{
  "env": {},
  "globals": {},
  "rules": {
    "no-bitwise": "error",
    "eqeqeq": "error",
    "no-use-before-define": "error",
    "no-caller": "error",
    "no-new": "error",
    "no-undef": "error",
    "no-unused-vars": "error",
    "curly": [
      "error",
      "all"
    ],
    "keyword-spacing": "error",
    "space-before-blocks": [
      "error",
      "always"
    ],
    "wrap-iife": "error",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "no-spaced-func": "error",
    "one-var": [
      "error",
      "always"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "semi-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-empty": [
      "error",
      { "allowEmptyCatch": true }
    ],
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    "space-in-parens": [
      "error",
      "always"
    ],
    "quote-props": [
      "error",
      "as-needed",
      { "keywords": true }
    ],
    "no-underscore-dangle": "error",
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "space-unary-ops": [
      "error",
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": "error",
    "no-implicit-coercion": [
      "error",
      {
        "string": true,
        "boolean": false,
        "number": false
      }
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "no-with": "error",
    "no-multiple-empty-lines": "error",
    "no-mixed-spaces-and-tabs": "error",
    "operator-linebreak": [
      "error",
      "after"
    ],
    "dot-location": [
      "error",
      "property"
    ],
    "no-trailing-spaces": "error",
    "comma-dangle": [
      "error",
      "never"
    ],
    "brace-style": [
      "error",
      "1tbs",
      { "allowSingleLine": true }
    ],
    "eol-last": "error",
    "dot-notation": [
      "error",
      { "allowKeywords": false }
    ],
    "yoda": [
      "error",
      "never"
    ],
    "spaced-comment": [
      "error",
      "always"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "indent": [
      "error",
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "valid-jsdoc": [
      "error",
      {
        "requireParamDescription": false,
        "requireReturnDescription": false,
        "requireReturn": false
      }
    ]
  }
}
